{
  "name": "badges_server",
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@now/node-server" }],
  "routes": [{ "src": "/(.*)", "dest": "./index.js" }],
  "env": {
    "DB_USER": "@platzibadge-db-user",
    "DB_PASSWORD": "@platzibadge-db-password",
    "DB_HOST": "@platzibadge-db-host",
    "DB_NAME": "@platzibadge-db-name",
    "CORS": "*"
  }
}
